<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/lytFragmentFlightPassSearch"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_app_background"
    android:clickable="true"
    android:orientation="vertical">

    <android.support.v4.widget.NestedScrollView
        android:id="@+id/svParent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:background="@color/color_app_background"
        android:scrollbars="none">

        <RelativeLayout
            android:id="@+id/lytMain"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/dp_20"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_20">

            <LinearLayout
                android:id="@+id/lytError"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_0"
                android:layout_marginRight="@dimen/dp_0"
                android:layout_marginTop="@dimen/dp_20"
                android:background="@color/color_moccasin"
                android:orientation="horizontal"
                android:padding="@dimen/dp_10"
                android:visibility="gone">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/error4_icon" />

                <LinearLayout
                    android:id="@+id/lytErrorMessage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_5"
                    android:orientation="vertical">


                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:id="@+id/lytSelectFligthPass"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lytError"
                android:layout_marginTop="@dimen/dp_10"
                android:clickable="true"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_30"
                    android:clickable="true"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/dp_27"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/redeemselectflightpass" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_15"
                        android:orientation="vertical">

                        <com.optiontown.app.view.customview.OTTextView
                            android:id="@+id/txtAirlineName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_5"
                            android:background="@android:color/transparent"
                            android:text="@string/string_select_flight_pass"
                            android:textColor="@color/color_font_black"
                            android:textSize="@dimen/size_font_13"
                            android:textStyle="normal" />

                    </LinearLayout>

                </LinearLayout>

                <View
                    android:id="@+id/view1"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_below="@+id/lytSelectFligthPass"
                    android:layout_marginTop="@dimen/dp_8"
                    android:background="@color/color_common_border" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/lytRadioButtons"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lytSelectFligthPass"
                android:layout_marginTop="@dimen/dp_20"
                android:clickable="true"
                android:orientation="horizontal">

                <RadioGroup
                    android:id="@+id/rGroupTrips"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <RadioButton
                        android:id="@+id/rbOneWay"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@color/caldroid_lighter_gray"
                        android:checked="true"
                        android:paddingBottom="@dimen/dp_5"
                        android:paddingRight="@dimen/dp_10"
                        android:paddingTop="@dimen/dp_5"
                        android:text="One Way"
                        android:textSize="@dimen/size_font_13"
                        android:visibility="gone" />

                    <RadioButton
                        android:id="@+id/rbRoundTrip"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:checked="false"
                        android:paddingBottom="@dimen/dp_5"
                        android:paddingRight="@dimen/dp_10"
                        android:paddingTop="@dimen/dp_5"
                        android:text="Return Trip"
                        android:textSize="@dimen/size_font_13"
                        android:visibility="gone" />

                    <RadioButton
                        android:id="@+id/rbOpenJaw"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:checked="false"
                        android:paddingBottom="@dimen/dp_5"
                        android:paddingRight="@dimen/dp_10"
                        android:paddingTop="@dimen/dp_5"
                        android:text="Open Jaw"
                        android:textSize="@dimen/size_font_13"
                        android:visibility="gone" />
                </RadioGroup>
            </LinearLayout>


            <com.optiontown.app.view.customview.OTTextView
                android:id="@+id/txtFlight1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lytRadioButtons"
                android:layout_marginTop="@dimen/dp_15"
                android:background="@android:color/transparent"
                android:text="@string/string_flight1"
                android:textColor="@color/color_gray_dark"
                android:textSize="@dimen/dp_15"
                android:visibility="gone" />


            <LinearLayout
                android:id="@+id/lytFrom"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txtFlight1"
                android:layout_marginTop="@dimen/dp_15"
                android:clickable="true"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/lytSubFrom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/dp_27"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/airline" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_15"
                        android:orientation="vertical">

                        <com.optiontown.app.view.customview.OTTextView
                            android:id="@+id/txtFrom"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_5"
                            android:background="@android:color/transparent"
                            android:text="@string/string_from"
                            android:textColor="@color/color_font_black"
                            android:textSize="@dimen/size_font_13"
                            android:textStyle="normal" />

                    </LinearLayout>

                </LinearLayout>

                <View
                    android:id="@+id/view2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginTop="@dimen/dp_8"
                    android:background="@color/color_common_border" />

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtHelpMeChooseFrom"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginTop="@dimen/dp_5"
                    android:background="@android:color/transparent"
                    android:text="Help Me Choose"
                    android:textColor="@color/color_font_OT_blue"
                    android:textSize="@dimen/size_font_12"
                    android:textStyle="normal" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lytTo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lytFrom"
                android:layout_marginTop="@dimen/dp_20"
                android:clickable="true"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/lytSubTo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/dp_27"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/arrive" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_15"
                        android:orientation="vertical">

                        <com.optiontown.app.view.customview.OTTextView
                            android:id="@+id/txtTo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_5"
                            android:background="@android:color/transparent"
                            android:text="@string/string_to"
                            android:textColor="@color/color_font_black"
                            android:textSize="@dimen/size_font_13"
                            android:textStyle="normal" />

                    </LinearLayout>

                </LinearLayout>

                <View
                    android:id="@+id/view3"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginTop="@dimen/dp_8"
                    android:background="@color/color_common_border" />

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtHelpMeChooseTo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginTop="@dimen/dp_5"
                    android:background="@android:color/transparent"
                    android:text="Help Me Choose"
                    android:textColor="@color/color_font_OT_blue"
                    android:textSize="@dimen/size_font_12"
                    android:textStyle="normal" />
            </LinearLayout>

            <com.optiontown.app.view.customview.OTTextView
                android:id="@+id/txtFlight2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lytTo"
                android:layout_marginTop="@dimen/dp_5"
                android:background="@android:color/transparent"
                android:text="@string/string_flight2"
                android:textColor="@color/color_gray_dark"
                android:textSize="@dimen/dp_15"
                android:visibility="gone" />

            <LinearLayout
                android:id="@+id/lytFromRepeat"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txtFlight2"
                android:layout_marginTop="@dimen/dp_15"
                android:clickable="true"
                android:orientation="vertical"
                android:visibility="gone">

                <LinearLayout
                    android:id="@+id/lytSubFromRepeat"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/dp_27"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/airline" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_15"
                        android:orientation="vertical">

                        <com.optiontown.app.view.customview.OTTextView
                            android:id="@+id/txtFromRepeat"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_5"
                            android:background="@android:color/transparent"
                            android:text="@string/string_from"
                            android:textColor="@color/color_font_black"
                            android:textSize="@dimen/size_font_13"
                            android:textStyle="normal" />

                    </LinearLayout>

                </LinearLayout>

                <View
                    android:id="@+id/view2Repeat"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginTop="@dimen/dp_8"
                    android:background="@color/color_common_border" />

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtHelpMeChooseFromRepeat"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginTop="@dimen/dp_5"
                    android:background="@android:color/transparent"
                    android:text="Help Me Choose"
                    android:textColor="@color/color_font_OT_blue"
                    android:textSize="@dimen/size_font_12"
                    android:textStyle="normal" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lytToRepeat"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lytFromRepeat"
                android:layout_marginTop="@dimen/dp_20"
                android:clickable="true"
                android:orientation="vertical"
                android:visibility="gone">

                <LinearLayout
                    android:id="@+id/lytSubToRepeat"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/dp_27"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/arrive" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_15"
                        android:orientation="vertical">

                        <com.optiontown.app.view.customview.OTTextView
                            android:id="@+id/txtToRepeat"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_5"
                            android:background="@android:color/transparent"
                            android:text="@string/string_to"
                            android:textColor="@color/color_font_black"
                            android:textSize="@dimen/size_font_13"
                            android:textStyle="normal" />

                    </LinearLayout>

                </LinearLayout>

                <View
                    android:id="@+id/view3Repeat"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginTop="@dimen/dp_8"
                    android:background="@color/color_common_border" />

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtHelpMeChooseToRepeat"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginTop="@dimen/dp_5"
                    android:background="@android:color/transparent"
                    android:text="Help Me Choose"
                    android:textColor="@color/color_font_OT_blue"
                    android:textSize="@dimen/size_font_12"
                    android:textStyle="normal" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/lytDepartAndReturn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lytToRepeat"
                android:layout_marginTop="@dimen/dp_20"
                android:clickable="true"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/lytDepart"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dp_10"
                        android:layout_weight="2"
                        android:clickable="true"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <LinearLayout
                            android:id="@+id/lytSubDepart"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clickable="true"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/dp_27"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:adjustViewBounds="true"
                                android:src="@drawable/depart_date" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_15"
                                android:orientation="vertical">

                                <com.optiontown.app.view.customview.OTTextView
                                    android:id="@+id/txtDepart"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dp_5"
                                    android:background="@android:color/transparent"
                                    android:text="@string/string_depart"
                                    android:textColor="@color/color_font_black"
                                    android:textSize="@dimen/size_font_13"
                                    android:textStyle="normal" />

                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:id="@+id/view5"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_1"
                            android:layout_marginTop="@dimen/dp_8"
                            android:background="@color/color_common_border" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lytReturn"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:clickable="true"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <LinearLayout
                            android:id="@+id/lytSubReturn"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clickable="true"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/dp_27"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:adjustViewBounds="true"
                                android:src="@drawable/depart_date" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_15"
                                android:orientation="vertical">

                                <com.optiontown.app.view.customview.OTTextView
                                    android:id="@+id/txtReturn"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dp_5"
                                    android:background="@android:color/transparent"
                                    android:text="@string/string_return"
                                    android:textColor="@color/color_font_black"
                                    android:textSize="@dimen/size_font_13"
                                    android:textStyle="normal" />

                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:id="@+id/view4"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_1"
                            android:layout_marginTop="@dimen/dp_8"
                            android:background="@color/color_common_border" />

                    </LinearLayout>

                </LinearLayout>

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtHelpMeChooseReturn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginTop="@dimen/dp_5"
                    android:background="@android:color/transparent"
                    android:text="Help Me Choose"
                    android:textColor="@color/color_font_OT_blue"
                    android:textSize="@dimen/size_font_12"
                    android:textStyle="normal" />
            </LinearLayout>


            <com.optiontown.app.view.customview.OTTextView
                android:id="@+id/txtTravelFlexibilityLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lytDepartAndReturn"
                android:layout_marginTop="@dimen/dp_20"
                android:background="@android:color/transparent"
                android:text="Travel Flexibility"
                android:textColor="@color/color_font_black"
                android:textSize="@dimen/dp_16"
                android:textStyle="bold" />


            <LinearLayout
                android:id="@+id/lytDepartAndReturnFlexibility"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txtTravelFlexibilityLabel"
                android:layout_marginTop="@dimen/dp_20"
                android:clickable="true"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/lytTravelFlexibility"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dp_10"
                        android:layout_weight="2"
                        android:clickable="true"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <LinearLayout
                            android:id="@+id/lytSubTravelFlexibility"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clickable="true"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/dp_27"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:adjustViewBounds="true"
                                android:src="@drawable/depart_date" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_15"
                                android:orientation="vertical">

                                <com.optiontown.app.view.customview.OTTextView
                                    android:id="@+id/txtTravelFlexibility"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dp_5"
                                    android:background="@android:color/transparent"
                                    android:text="@string/string_deapart_flexibility"
                                    android:textColor="@color/color_font_black"
                                    android:textSize="@dimen/size_font_13"
                                    android:textStyle="normal" />

                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:id="@+id/view6"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_1"
                            android:layout_marginTop="@dimen/dp_8"
                            android:background="@color/color_common_border" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lytReturnFlexibility"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:clickable="true"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <LinearLayout
                            android:id="@+id/lytSubReturnFlexibility"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clickable="true"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/dp_27"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:adjustViewBounds="true"
                                android:src="@drawable/depart_date" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_15"
                                android:orientation="vertical">

                                <com.optiontown.app.view.customview.OTTextView
                                    android:id="@+id/txtReturnFlexibility"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dp_5"
                                    android:background="@android:color/transparent"
                                    android:text="@string/string_return_flexibility"
                                    android:textColor="@color/color_font_black"
                                    android:textSize="@dimen/size_font_13"
                                    android:textStyle="normal" />

                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:id="@+id/view7"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_1"
                            android:layout_marginTop="@dimen/dp_8"
                            android:background="@color/color_common_border" />

                    </LinearLayout>

                </LinearLayout>

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtHelpMeChooseReturnFlexibity"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:background="@android:color/transparent"
                    android:text="Help Me Choose"
                    android:textColor="@color/color_font_OT_blue"
                    android:textSize="@dimen/size_font_12"
                    android:textStyle="normal" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/lytPassenger"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lytDepartAndReturnFlexibility"
                android:layout_marginTop="@dimen/dp_20"
                android:clickable="true"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/lytSubPassenger"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <com.optiontown.app.view.customview.OTTextView
                            android:id="@+id/txtpassengerlabel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@android:color/transparent"
                            android:text="Passenger(s)"
                            android:textColor="@color/color_gray_dark"
                            android:textSize="@dimen/size_font_12"
                            android:textStyle="normal" />

                        <com.optiontown.app.view.customview.OTTextView
                            android:id="@+id/txtPassenger"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_5"
                            android:background="@android:color/transparent"
                            android:text="Passenger"
                            android:textColor="@color/color_font_black"
                            android:textSize="@dimen/size_font_13"
                            android:textStyle="normal" />

                    </LinearLayout>

                </LinearLayout>

                <View
                    android:id="@+id/view8"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginTop="@dimen/dp_8"
                    android:background="@color/color_common_border" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/lytCabin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lytPassenger"
                android:layout_marginTop="@dimen/dp_30"
                android:clickable="true"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/lytSubCabin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:orientation="vertical">

                    <com.optiontown.app.view.customview.OTTextView
                        android:id="@+id/txtCabinLabel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:text="Cabin"
                        android:textColor="@color/color_gray_dark"
                        android:textSize="@dimen/size_font_12"
                        android:textStyle="normal" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <com.optiontown.app.view.customview.OTTextView
                            android:id="@+id/txtCabin"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_5"
                            android:background="@android:color/transparent"
                            android:text="Cabin"
                            android:textColor="@color/color_font_black"
                            android:textSize="@dimen/size_font_13"
                            android:textStyle="normal" />

                    </LinearLayout>

                </LinearLayout>

                <View
                    android:id="@+id/view9"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginTop="@dimen/dp_8"
                    android:background="@color/color_common_border" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/lytSearch"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lytCabin"
                android:layout_marginTop="@dimen/dp_35"
                android:clickable="true">

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtSearch"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rounded_small_corner_search"
                    android:gravity="center"
                    android:padding="@dimen/dp_10"
                    android:text="@string/string_search"
                    android:textColor="@color/color_font_white"
                    android:textSize="@dimen/size_font_18"
                    android:textStyle="bold" />

            </LinearLayout>


            <android.support.v4.widget.NestedScrollView
                android:id="@+id/svSelectFlightPass"
                android:layout_width="@dimen/dp_100"
                android:layout_height="@dimen/height_drop_down"
                android:layout_alignLeft="@+id/lytSelectFligthPass"
                android:layout_alignRight="@+id/lytSelectFligthPass"
                android:layout_below="@+id/lytSelectFligthPass"
                android:background="@drawable/rounded_dark_gray_stroke"
                android:scrollbars="vertical"
                android:visibility="gone">

                <com.optiontown.app.view.customview.InterceptedLinearLayout
                    android:id="@+id/lytFlightPassScroll"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/dp_10"

                    />
            </android.support.v4.widget.NestedScrollView>

            <android.support.v4.widget.NestedScrollView
                android:id="@+id/svFrom"
                android:layout_width="@dimen/dp_100"
                android:layout_height="@dimen/height_drop_down"
                android:layout_alignLeft="@+id/lytFrom"
                android:layout_alignRight="@+id/lytFrom"
                android:layout_below="@+id/lytFrom"
                android:background="@drawable/rounded_dark_gray_stroke"
                android:scrollbars="vertical"
                android:visibility="gone">

                <LinearLayout
                    android:id="@+id/lytFromScroll"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/dp_10"

                    />
            </android.support.v4.widget.NestedScrollView>

            <android.support.v4.widget.NestedScrollView
                android:id="@+id/svTo"
                android:layout_width="@dimen/dp_100"
                android:layout_height="@dimen/height_drop_down"
                android:layout_alignLeft="@+id/lytTo"
                android:layout_alignRight="@+id/lytTo"
                android:layout_below="@+id/lytTo"
                android:background="@drawable/rounded_dark_gray_stroke"
                android:scrollbars="vertical"
                android:visibility="gone">

                <LinearLayout
                    android:id="@+id/lytToScroll"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/dp_10"

                    />
            </android.support.v4.widget.NestedScrollView>

            <android.support.v4.widget.NestedScrollView
                android:id="@+id/svlytDepartFlexibility"
                android:layout_width="@dimen/dp_100"
                android:layout_height="@dimen/height_drop_down"
                android:layout_alignLeft="@+id/lytDepartAndReturnFlexibility"
                android:layout_alignRight="@+id/lytDepartAndReturnFlexibility"
                android:layout_below="@+id/lytDepartAndReturnFlexibility"
                android:layout_marginRight="@dimen/dp_100"
                android:background="@drawable/rounded_dark_gray_stroke"
                android:scrollbars="vertical"
                android:visibility="gone">

                <LinearLayout
                    android:id="@+id/lytDepartFlexibilityScroll"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/dp_10"

                    />
            </android.support.v4.widget.NestedScrollView>

            <android.support.v4.widget.NestedScrollView
                android:id="@+id/svlytReturnFlexibility"
                android:layout_width="@dimen/dp_100"
                android:layout_height="@dimen/height_drop_down"
                android:layout_alignLeft="@+id/lytDepartAndReturnFlexibility"
                android:layout_alignRight="@+id/lytDepartAndReturnFlexibility"
                android:layout_below="@+id/lytDepartAndReturnFlexibility"
                android:layout_marginLeft="@dimen/dp_100"
                android:background="@drawable/rounded_dark_gray_stroke"
                android:scrollbars="vertical"
                android:visibility="gone">

                <LinearLayout
                    android:id="@+id/lytReturnFlexibilityScroll"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/dp_10"

                    />
            </android.support.v4.widget.NestedScrollView>

            <android.support.v4.widget.NestedScrollView
                android:id="@+id/svPassenger"
                android:layout_width="@dimen/dp_100"
                android:layout_height="@dimen/height_drop_down"
                android:layout_alignLeft="@+id/lytPassenger"
                android:layout_alignRight="@+id/lytPassenger"
                android:layout_below="@+id/lytPassenger"
                android:background="@drawable/rounded_dark_gray_stroke"
                android:scrollbars="vertical"
                android:visibility="gone">

                <LinearLayout
                    android:id="@+id/lytPassengerSV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/dp_10"

                    />
            </android.support.v4.widget.NestedScrollView>


            <View
                android:id="@+id/view10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_160"
                android:layout_below="@+id/lytSearch" />

        </RelativeLayout>


    </android.support.v4.widget.NestedScrollView>

    <LinearLayout
        android:id="@+id/lytCloseDone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="@dimen/dp_20"
        android:orientation="horizontal"
        android:visibility="visible">

        <com.optiontown.app.view.customview.OTTextView
            android:id="@+id/txtClose"
            android:layout_width="@dimen/dp_0"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@color/color_gray_light"
            android:gravity="center"
            android:padding="@dimen/dp_10"
            android:text="Close"
            android:textColor="@color/color_font_black"
            android:textSize="@dimen/size_font_18"
            android:textStyle="bold" />

        <com.optiontown.app.view.customview.OTTextView
            android:id="@+id/txtDone"
            android:layout_width="@dimen/dp_0"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@color/color_red"
            android:gravity="center"
            android:padding="@dimen/dp_10"
            android:text="Done"
            android:textColor="@color/color_font_white"
            android:textSize="@dimen/size_font_18"
            android:textStyle="bold" />

    </LinearLayout>

    <RelativeLayout
        android:id="@+id/bottom_bar_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="6dp"
        android:layout_marginStart="6dp">

        <include
            android:id="@+id/app_bottom_bar_fpo_redeem_more"
            layout="@layout/app_bottom_bar_fpo_redeem_more"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true" />

        <include
            android:id="@+id/app_bottom_bar_fpo_redeem"
            layout="@layout/app_bottom_bar_fpo_redeem"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true" />

    </RelativeLayout>

</RelativeLayout>