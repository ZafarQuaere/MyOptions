<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_app_background"
    android:clickable="true"
    android:orientation="vertical"
    android:padding="@dimen/dp_10"
    android:scrollbars="none">

    <RelativeLayout
        android:id="@+id/lytLegProductReview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="@dimen/dp_5">

        <LinearLayout
            android:id="@+id/lyt_head_table"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_5"
            android:background="@drawable/background_suggestion_airlines"
            android:orientation="vertical">

            <com.optiontown.app.view.customview.OTTextView
                android:id="@+id/txtUtoPriceSummaryLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_10"
                android:paddingBottom="@dimen/dp_5"
                android:paddingTop="@dimen/dp_5"
                android:text="@string/string_uto_price_summary"
                android:textColor="@color/color_font_black"
                android:textSize="@dimen/size_font_15"
                android:textStyle="bold" />

            <!-- if Partial is not ON -->
            <LinearLayout
                android:id="@+id/lytHeaderThreeColumn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/empty_seat_search_header_bg"
                android:orientation="horizontal"
                android:visibility="visible">

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtFlightsLabel"
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center|center_vertical"
                    android:padding="@dimen/dp_5"
                    android:text="Flight"
                    android:textColor="@color/color_font_black"
                    android:textSize="@dimen/size_font_12" />

                <View
                    android:id="@+id/viewHead"
                    android:layout_width="@dimen/dp_1"
                    android:layout_height="match_parent"
                    android:background="@color/color_common_border" />

                <LinearLayout
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <com.optiontown.app.view.customview.OTTextView
                        android:id="@+id/txtSignUpPriceLabel"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:paddingLeft="@dimen/dp_5"
                        android:paddingRight="@dimen/dp_5"
                        android:paddingTop="@dimen/dp_5"
                        android:singleLine="false"
                        android:text="Sign Up Price UTO"
                        android:textColor="@color/color_font_black"
                        android:textSize="@dimen/size_font_12" />

                    <com.optiontown.app.view.customview.OTTextView
                        android:id="@+id/txtSignUpPriceRefundLabel"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:paddingLeft="@dimen/dp_5"
                        android:paddingRight="@dimen/dp_5"
                        android:singleLine="false"
                        android:text="(non refundable)"
                        android:textColor="@color/color_font_black"
                        android:textSize="@dimen/size_font_12" />
                </LinearLayout>

                <View
                    android:layout_width="@dimen/dp_1"
                    android:layout_height="match_parent"
                    android:background="@color/color_common_border" />

                <LinearLayout
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <com.optiontown.app.view.customview.OTTextView
                        android:id="@+id/txtUpgradePriceLabel"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal|center_vertical"
                        android:paddingLeft="@dimen/dp_5"
                        android:paddingRight="@dimen/dp_5"
                        android:paddingTop="@dimen/dp_5"
                        android:singleLine="false"
                        android:text="EmptySeat Price"
                        android:textColor="@color/color_font_black"
                        android:textSize="@dimen/size_font_12" />

                    <com.optiontown.app.view.customview.OTTextView
                        android:id="@+id/txtUpgradePriceRefundLabel"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:paddingLeft="@dimen/dp_5"
                        android:paddingRight="@dimen/dp_5"
                        android:singleLine="false"
                        android:text="(will be refunded if not upgraded)"
                        android:textColor="@color/color_font_black"
                        android:textSize="@dimen/size_font_12" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/lytHeaderTwoColumn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/empty_seat_search_header_bg"
                android:orientation="horizontal"
                android:visibility="gone">

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtFlightsLabelTwoColumn"
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="@dimen/dp_5"
                    android:text="Flight"
                    android:textColor="@color/color_font_black"
                    android:textSize="@dimen/size_font_12" />

                <View
                    android:id="@+id/viewHead1"
                    android:layout_width="@dimen/dp_1"
                    android:layout_height="match_parent"
                    android:background="@color/color_common_border" />

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtUpgradePriceLabelTwoColumn"
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:gravity="center"
                    android:padding="@dimen/dp_5"
                    android:singleLine="false"
                    android:text="Upgrade Price (will be refunded if not upgraded)"
                    android:textColor="@color/color_font_black"
                    android:textSize="@dimen/size_font_12" />

            </LinearLayout>

            <android.support.v7.widget.RecyclerView
                android:id="@+id/recycleViewReview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </android.support.v7.widget.RecyclerView>


            <LinearLayout
                android:id="@+id/lytTotalPrice"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/color_font_white_light"
                android:orientation="horizontal">

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtTotalPriceLabel"
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_weight="1"
                    android:padding="@dimen/dp_5"
                    android:text="Total Price"
                    android:textColor="@color/color_font_black"
                    android:textSize="@dimen/size_font_15"
                    android:textStyle="bold" />


                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtTotalPrice"
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:padding="@dimen/dp_5"
                    android:singleLine="false"
                    android:text="298 "
                    android:textColor="@color/color_font_black"
                    android:textSize="@dimen/size_font_15"
                    android:textStyle="bold" />
            </LinearLayout>

            <com.optiontown.app.view.customview.OTTextView
                android:id="@+id/txtSignUpPriceInfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="@dimen/dp_2"
                android:text="*Singup Price"
                android:textSize="@dimen/size_font_10" />

            <com.optiontown.app.view.customview.OTTextView
                android:id="@+id/txtOptionPriceInfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="@dimen/dp_2"
                android:text="*Upgrade Price"
                android:textSize="@dimen/size_font_10" />

            <LinearLayout

                android:id="@+id/lyPaxCount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lyt_head_table"
                android:orientation="horizontal">

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtEligiblePassengerLabel"
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_weight="1"
                    android:paddingBottom="@dimen/dp_10"
                    android:paddingLeft="@dimen/dp_5"
                    android:paddingRight="@dimen/dp_5"
                    android:paddingTop="@dimen/dp_10"
                    android:singleLine="false"
                    android:text="Eligible Passenger"
                    android:textColor="@color/color_font_black"
                    android:textSize="@dimen/size_font_12" />

                <View
                    android:layout_width="@dimen/dp_1"
                    android:layout_height="match_parent"
                    android:background="@color/color_common_border" />

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtNoOfPassenger"
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="2"
                    android:gravity="end"
                    android:paddingBottom="@dimen/dp_5"
                    android:paddingRight="@dimen/dp_10"
                    android:paddingTop="@dimen/dp_5"
                    android:singleLine="false"
                    android:text="  "
                    android:textColor="@color/color_font_black"
                    android:textSize="@dimen/size_font_12" />

            </LinearLayout>
            <View
                android:id="@+id/viewDummy"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_common_border" />

            <LinearLayout
                android:id="@+id/lytTotalAmountPay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewDummy"
                android:orientation="horizontal">

                <com.optiontown.app.view.customview.OTTextViewHtml
                    android:id="@+id/txtTotalAmountToPayLabel"
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_weight="1"
                    android:paddingBottom="@dimen/dp_10"
                    android:paddingLeft="@dimen/dp_5"
                    android:paddingRight="@dimen/dp_5"
                    android:paddingTop="@dimen/dp_10"
                    android:text="Total Amount to pay now"
                    android:textColor="@color/color_font_black"
                    android:textSize="@dimen/size_font_12" />

                <View
                    android:layout_width="@dimen/dp_1"
                    android:layout_height="match_parent"
                    android:background="@color/color_common_border" />

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtTotalAmountToPay"
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="2"
                    android:gravity="center|end"
                    android:paddingBottom="@dimen/dp_5"
                    android:paddingRight="@dimen/dp_10"
                    android:paddingTop="@dimen/dp_5"
                    android:singleLine="false"
                    android:text=" "
                    android:textColor="@color/color_font_black"
                    android:textSize="@dimen/size_font_12" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/lytUpgradePassCreditSummary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lyt_head_table"
            android:layout_marginTop="@dimen/dp_10"
            android:orientation="vertical">

            <com.optiontown.app.view.customview.OTTextView
                android:id="@+id/txtUpCreditSummaryLable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="@dimen/dp_10"
                android:paddingTop="@dimen/dp_5"
                android:text="Upgrade Pass Credit Summary"
                android:textColor="@color/color_font_blue"
                android:textSize="@dimen/size_font_14"
                android:textStyle="bold" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/background_suggestion_airlines"
                android:orientation="horizontal"
                android:padding="@dimen/dp_2">

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtFlightLable"
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center|center_vertical"
                    android:paddingLeft="@dimen/dp_5"
                    android:text="Flight"
                    android:textColor="@color/color_font_black"
                    android:textSize="@dimen/size_font_13"
                    android:textStyle="bold" />

                <View
                    android:layout_width="@dimen/dp_1"
                    android:layout_height="match_parent"
                    android:background="@color/color_common_border" />

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txtUpCreditRequiredLabel"
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:paddingLeft="@dimen/dp_2"
                    android:text="Upgrade Pass Credit Required"
                    android:textColor="@color/color_font_black"
                    android:textSize="@dimen/size_font_13"
                    android:textStyle="bold" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/lytUpgradePassCreditSummaryRow"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_common_border" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/background_suggestion_airlines"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <com.optiontown.app.view.customview.OTTextView
                        android:id="@+id/txtTotalUpCreditRqrdLabel"
                        android:layout_width="@dimen/dp_0"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:padding="@dimen/dp_2"
                        android:text="Total Upgrade pass credit required"
                        android:textColor="@color/color_font_black"
                        android:textSize="@dimen/size_font_13" />

                    <View
                        android:layout_width="@dimen/dp_1"
                        android:layout_height="match_parent"
                        android:background="@color/color_common_border" />

                    <com.optiontown.app.view.customview.OTTextView
                        android:id="@+id/txtTotalUpCreditRequired"
                        android:layout_width="@dimen/dp_0"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:paddingLeft="@dimen/dp_2"
                        android:text=" 2 "
                        android:textColor="@color/color_font_black"
                        android:textSize="@dimen/size_font_13" />
                </LinearLayout>

            </LinearLayout>


        </LinearLayout>


        <com.optiontown.app.view.customview.OTTextView
            android:id="@+id/txtContactInfoLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lytUpgradePassCreditSummary"
            android:layout_marginTop="@dimen/dp_15"
            android:text="@string/string_contact_information"
            android:textColor="@color/color_font_blue"
            android:textSize="@dimen/size_font_14"
            android:textStyle="bold" />

        <com.optiontown.app.view.customview.OTTextView
            android:id="@+id/txtEmailLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/btnContinueToNextSteps"
            android:layout_alignStart="@+id/btnContinueToNextSteps"
            android:layout_below="@+id/txtContactInfoLabel"
            android:layout_marginTop="@dimen/dp_10"
            android:text="@string/string_contact_email_star"
            android:textColor="@color/color_font_black"
            android:textSize="@dimen/size_font_12" />

        <com.optiontown.app.view.customview.OTEditText
            android:id="@+id/edtEmail"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_35"
            android:layout_below="@+id/txtEmailLabel"
            android:layout_marginTop="@dimen/dp_5"
            android:background="@drawable/rounded_stroke_light_blue"
            android:inputType="textEmailAddress"
            android:maxLength="60"
            android:padding="@dimen/dp_6"
            android:singleLine="true"
            android:textColor="@color/color_font_black"
            android:textSize="@dimen/size_font_14" />

        <com.optiontown.app.view.customview.OTTextView
            android:id="@+id/txtCellMobileLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/edtEmail"
            android:layout_marginTop="@dimen/dp_10"
            android:text="@string/string_cell_mobile"
            android:textColor="@color/color_font_black"
            android:textSize="@dimen/size_font_12" />

        <com.optiontown.app.view.customview.OTTextView
            android:id="@+id/txtMobileCC"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_35"
            android:layout_below="@+id/txtCellMobileLabel"
            android:layout_marginRight="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_5"
            android:background="@drawable/bg_spinner"
            android:gravity="left|center_vertical"
            android:padding="@dimen/dp_5"
            android:text="USA (+1)"
            android:textColor="@color/color_font_black"
            android:textSize="@dimen/size_font_11"
            android:textStyle="normal" />

        <com.optiontown.app.view.customview.OTEditText
            android:id="@+id/edtCellMobile"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_35"
            android:layout_below="@+id/txtCellMobileLabel"
            android:layout_marginTop="@dimen/dp_5"
            android:layout_toRightOf="@+id/txtMobileCC"
            android:background="@drawable/rounded_stroke_light_blue"
            android:inputType="number"
            android:maxLength="14"
            android:padding="@dimen/dp_6"
            android:singleLine="true"
            android:textColor="@color/color_font_black"
            android:textSize="@dimen/size_font_14" />

        <com.optiontown.app.view.customview.OTTextView
            android:id="@+id/txtOtherPhoneLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtMobileCC"
            android:layout_marginTop="@dimen/dp_10"
            android:text="@string/string_other_phone"
            android:textColor="@color/color_font_black"
            android:textSize="@dimen/size_font_12" />

        <com.optiontown.app.view.customview.OTTextView
            android:id="@+id/txtOtherPhoneCode"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_35"
            android:layout_below="@+id/txtOtherPhoneLabel"
            android:layout_marginRight="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_5"
            android:background="@drawable/bg_spinner"
            android:gravity="left|center_vertical"
            android:padding="@dimen/dp_5"
            android:text="USA (+1)"
            android:textColor="@color/color_font_black"
            android:textSize="@dimen/size_font_11"
            android:textStyle="normal" />

        <com.optiontown.app.view.customview.OTEditText
            android:id="@+id/edtOtherPhone"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_35"
            android:layout_below="@+id/txtOtherPhoneLabel"
            android:layout_marginTop="@dimen/dp_5"
            android:layout_toRightOf="@+id/txtOtherPhoneCode"
            android:background="@drawable/rounded_stroke_light_blue"
            android:inputType="number"
            android:maxLength="14"
            android:padding="@dimen/dp_6"
            android:singleLine="true"
            android:textColor="@color/color_font_black"
            android:textSize="@dimen/size_font_14" />


        <com.optiontown.app.view.customview.OTTextView
            android:id="@+id/txtPaymentModeLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtOtherPhoneCode"
            android:layout_marginTop="@dimen/dp_20"
            android:text="@string/string_payment_mode"
            android:textColor="@color/color_font_blue"
            android:textSize="@dimen/size_font_14"
            android:textStyle="bold" />

        <RelativeLayout
            android:id="@+id/lytRadioGrp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtPaymentModeLabel"
            android:layout_marginTop="@dimen/dp_10"
            android:background="@drawable/empty_seat_search_header_bg"
            android:orientation="vertical">

            <RadioGroup
                android:id="@+id/radio_grp_payment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="@dimen/dp_4">

            </RadioGroup>

            <com.optiontown.app.view.customview.OTTextView
                android:id="@+id/txtPayWithCard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/radio_grp_payment"
                android:layout_marginBottom="@dimen/dp_5"
                android:layout_marginLeft="@dimen/dp_5"
                android:layout_marginRight="@dimen/dp_10"
                android:background="@color/color_app_background"
                android:padding="@dimen/dp_5"
                android:text="Pay with Credit Card"
                android:textColor="@color/color_red"
                android:textSize="@dimen/size_font_11" />
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/lytPaypalCheck"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lytRadioGrp"
            android:orientation="vertical"
            android:visibility="gone">

            <com.optiontown.app.view.customview.OTTextView
                android:id="@+id/txtPaypayCheckLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_5"
                android:text=""
                android:textColor="@color/color_font_blue"
                android:textSize="@dimen/size_font_13"
                android:textStyle="bold" />

            <com.optiontown.app.view.customview.OTTextView
                android:id="@+id/txtPaypalDescription"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_8"
                android:text=""
                android:textColor="@color/color_font_black"
                android:textSize="@dimen/size_font_12" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lytMultiProduct"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lytPaypalCheck"
            android:layout_marginTop="@dimen/dp_10"
            android:background="@drawable/multiproduct_background"
            android:orientation="vertical"
            android:visibility="gone">

            <com.optiontown.app.view.customview.OTTextView
                android:id="@+id/txtFreeSignupLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/dp_5"
                android:text="FREE Sign-up - Empty Seat Option (ESo)"
                android:textColor="@color/color_font_blue"
                android:textSize="@dimen/size_font_12"
                android:textStyle="bold" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_common_border"></View>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="6.5"
                    android:gravity="center"
                    android:orientation="vertical">

                    <com.android.volley.toolbox.NetworkImageView
                        android:id="@+id/imgMultiPro"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingTop="@dimen/dp_4"
                        android:src="@drawable/empty_seat2by4" />

                    <com.optiontown.app.view.customview.OTTextView
                        android:id="@+id/txtNumberOfRow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/dp_5"
                        android:text="1 Row"
                        android:textColor="@color/color_font_blue"
                        android:textSize="@dimen/size_font_12"
                        android:textStyle="bold" />
                </LinearLayout>

                <View
                    android:layout_width="@dimen/dp_1"
                    android:layout_height="match_parent"
                    android:background="@color/color_common_border"></View>

                <LinearLayout
                    android:layout_width="@dimen/dp_0"
                    android:layout_height="match_parent"
                    android:layout_weight="3.5"
                    android:gravity="center"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <com.optiontown.app.view.customview.OTTextView
                            android:id="@+id/txtSelectLabel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Select"
                            android:textColor="@color/color_font_black"
                            android:textSize="@dimen/size_font_10" />

                        <CheckBox
                            android:id="@+id/cbSelect"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:checked="false"
                            android:text="₹ 0*"
                            android:textColor="@color/color_red" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/dp_2"
                        android:orientation="horizontal">

                        <com.optiontown.app.view.customview.OTTextView
                            android:id="@+id/txtSaveLabel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Save "
                            android:textColor="@color/color_font_black"
                            android:textSize="@dimen/size_font_10" />

                        <com.optiontown.app.view.customview.OTTextView
                            android:id="@+id/txtSelectAmount"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="₹ 1*"
                            android:textColor="@color/color_font_red"
                            android:textSize="@dimen/size_font_12" />
                    </LinearLayout>
                </LinearLayout>


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_common_border"></View>

            <com.optiontown.app.view.customview.OTTextView
                android:id="@+id/txtInformationLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/dp_5"
                android:text="*Get Empty Seats if not upgraded
Empty Seat Price INR 1000*
(per passenger per flight)"
                android:textColor="@color/color_font_black"
                android:textSize="@dimen/size_font_13"
                android:textStyle="normal" />

            <com.optiontown.app.view.customview.OTTextView
                android:id="@+id/txtViewRulesLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/dp_5"
                android:text="view rules"
                android:textColor="@color/color_font_blue"
                android:textSize="@dimen/size_font_11"
                android:textStyle="normal" />

            <LinearLayout
                android:id="@+id/lytMultiRules"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#D9EEFF"
                android:orientation="vertical"
                android:visibility="gone" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lytCheckTerms"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lytMultiProduct"
            android:layout_marginTop="@dimen/dp_20"
            android:orientation="vertical">


            <RelativeLayout
                android:id="@+id/rlytCheckBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible">

                <CheckBox
                    android:id="@+id/chkTerms"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <com.optiontown.app.view.customview.OTTextView
                    android:id="@+id/txt_plz_chk_box"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/chkTerms"
                    android:gravity="center_vertical"
                    android:text="@string/string_please_check_box_to_confirm_your_acceptance_of"
                    android:textColor="@color/color_font_black"
                    android:textSize="@dimen/size_font_12" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/txt_plz_chk_box"
                    android:layout_below="@+id/txt_plz_chk_box">

                    <com.optiontown.app.view.customview.OTTextView
                        android:id="@+id/txtUtoRules"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/txt_plz_chk_box"
                        android:text=" UTo Rules"
                        android:textColor="@color/color_font_blue"
                        android:textSize="@dimen/size_font_12" />

                    <com.optiontown.app.view.customview.OTTextView
                        android:id="@+id/txtAnd"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/txtUtoRules"
                        android:ellipsize="end"
                        android:lines="1"
                        android:text=" and "
                        android:textColor="@color/color_font_black"
                        android:textSize="@dimen/size_font_12" />

                    <com.optiontown.app.view.customview.OTTextView
                        android:id="@+id/txtTermsOfService"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/txtAnd"
                        android:ellipsize="end"
                        android:text="@string/string_terms_of_service"
                        android:textColor="@color/color_font_blue"
                        android:textSize="@dimen/size_font_12" />

                    <com.optiontown.app.view.customview.OTTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/txtTermsOfService"
                        android:ellipsize="end"
                        android:lines="2"
                        android:text=" before you continue. "
                        android:textColor="@color/color_font_black"
                        android:textSize="@dimen/size_font_12" />
                </LinearLayout>

            </RelativeLayout>


        </LinearLayout>


        <Button
            android:id="@+id/btnContinueToNextSteps"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_below="@+id/lytCheckTerms"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_5"
            android:layout_marginTop="@dimen/dp_10"
            android:background="@drawable/rounded_small_corner_search"
            android:gravity="center"
            android:padding="@dimen/dp_10"
            android:text="@string/string_proceed_to_payment"
            android:textAllCaps="false"
            android:textColor="@color/color_font_white"
            android:textSize="@dimen/size_font_16"
            android:textStyle="normal" />

        <ScrollView
            android:id="@+id/scrollPrimaryCountryCode"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/height_drop_down"
            android:layout_alignLeft="@+id/txtMobileCC"
            android:layout_alignRight="@+id/txtMobileCC"
            android:layout_below="@+id/txtMobileCC"
            android:layout_marginTop="@dimen/dp_3"
            android:background="@drawable/rounded_dark_gray_stroke"
            android:focusableInTouchMode="false"
            android:scrollbars="vertical"
            android:visibility="gone">

            <LinearLayout
                android:id="@+id/lytPhonExtension"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="@dimen/dp_10" />
        </ScrollView>

        <ScrollView
            android:id="@+id/scrollOtherCountryCode"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/height_drop_down"
            android:layout_alignLeft="@+id/txtOtherPhoneCode"
            android:layout_alignRight="@+id/txtOtherPhoneCode"
            android:layout_below="@+id/txtOtherPhoneCode"
            android:layout_marginTop="@dimen/dp_3"
            android:background="@drawable/rounded_dark_gray_stroke"
            android:focusableInTouchMode="false"
            android:scrollbars="vertical"
            android:visibility="gone">

            <LinearLayout
                android:id="@+id/lytOtherPhonExtension"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="@dimen/dp_10" />
        </ScrollView>

    </RelativeLayout>


</ScrollView>